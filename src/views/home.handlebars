{{#each productos}}
    <div>
        <h2>{{this.title}}</h2>
        <p>{{this.description}}</p>
        <img src={{this.thumbnail}} alt="Imagen del producto">
        <p>Precio: {{this.price}}</p>
        <p>Stock: {{this.stock}}</p>
        <p>ID: {{this.id}}</p>
    </div>
{{/each}}

<script src="/socket.io/socket.io.js"></script>
<script src="static/js/index.js"></script>